{
  "base_keymap": "VSCode",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 15,
  "buffer_font_size": 12.0,
  "buffer_line_height": { "custom": 1.6 },
  "tabs": {
    "close_position": "left",
    "file_icons": true,
    "git_status": true,
    "activate_on_close": "neighbour"
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Github Dark"
  },
  "languages": {
    "TypeScript": {
      "code_actions_on_format": {
        "source.organizeImports": true
      }
    },
    "TSX": {
      "code_actions_on_format": {
        "source.organizeImports": true
      }
    }
  },
  "git": {
    "inline_blame": {
      "enabled": true,
      "delay_ms": 0,
      "show_commit_summary": true
    }
  },
  "indent_guides": {
    "coloring": "indent_aware"
  },
  "hard_tabs": false,
  "journal": {
    "path": "~/Documents/sync/journal",
    "hour_format": "hour24"
  },
  "tab_size": 2,
  "terminal": {
    "line_height": { "custom": 1.6 },
    "font_size": 12
  }
}
